DividerLine {
    color: #1AFFFFFF;
}

QScrollArea {
    background: transparent;
    border: none;
    padding: 6px;
}

QWidget#scrollAreaWidgetContents {
    background: red;
}

QStackedWidget {
    background: transparent;
}

QDoubleSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: right;
    background-color: transparent;
    image: url(:svg/icons/chevron_right_16_filled_white.svg);
    padding: 4px;
    border-radius: 4px;
}

QDoubleSpinBox::up-button:hover {
    background-color: #1AFFFFFF;
}

QDoubleSpinBox::up-button:pressed {
    background-color: #10FFFFFF;
}

QDoubleSpinBox::down-button {
    subcontrol-origin: margin;
    subcontrol-position: left;
    background-color: transparent;
    image: url(:svg/icons/chevron_left_16_filled_white.svg);
    padding: 4px;
    border-radius: 4px;
}

QDoubleSpinBox::down-button:hover {
    background-color: #1AFFFFFF;
}

QDoubleSpinBox::down-button:pressed {
    background-color: #10FFFFFF;
}

TaskWindow {
    background-color: #202122;
}

PanelView {
    background: #2A2B2C;
    border: 1px solid rgb(20, 20, 20);
    border-radius: 6px;
}

PanelView[activePanel="true"] {
    border: 1px solid rgb(126, 149, 199);
}