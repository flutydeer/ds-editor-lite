QPushButton {
    background: #10FFFFFF;
    border: none;
    border-radius: 6px;
    color: #f2f2f2;
    padding: 6px 12px;
}

QPushButton:hover {
    background-color: #1AFFFFFF;
}

QPushButton:pressed {
    color: rgb(191, 191, 191);
    background-color: #10FFFFFF;
}

QPushButton:disabled {
    color: rgb(127, 127, 127);
    background-color: rgb(64, 64, 64)
}

Button {
    background: #10FFFFFF;
    border: none;
    border-radius: 6px;
    color: #f2f2f2;
    padding: 6px 12px;
}

Button:hover {
    background-color: #1AFFFFFF;
}

Button:pressed {
    color: rgb(191, 191, 191);
    background-color: #10FFFFFF;
}

Button:disabled {
    color: rgb(127, 127, 127);
    background-color: rgb(64, 64, 64)
}

AccentButton {
    background: rgb(155, 186, 255);
    border: none;
    border-radius: 6px;
    color: rgb(0, 28, 92);
    padding: 6px 12px;
}

AccentButton:hover {
    color: rgb(0, 28, 92);
    background-color: rgb(169, 196, 255);
}

AccentButton:pressed {
    color: rgba(0, 28, 92, 127);
    background-color: rgb(139, 175, 255);
}

AccentButton:disabled {
    color: rgb(166, 166, 166);
    background-color: rgb(205, 205, 205)
}

QComboBox {
    background: #10FFFFFF;
    border: none;
    border-radius: 6px;
    color: rgb(242, 242, 242);
    padding: 5px 12px;
}

QComboBox:hover {
    background-color: #1AFFFFFF;
}

QComboBox:disabled {
    color: rgb(127, 127, 127);
    background-color: rgb(64, 64, 64)
}

QComboBox QAbstractItemView {
    outline: 0px;
    border: 1px solid #80606060;
    color: #F0F0F0;
    background-color: #202122;
    border: 1px solid #606060;
    selection-background-color: #1AFFFFFF;
    border-radius: 6px;
    padding: 4px;
}

QComboBox QAbstractItemView:item {
    padding: 3px;
    border-radius: 4px;
    border: none;
}

QComboBox QAbstractItemView::item:selected {
    background-color: #1AFFFFFF;
}

QComboBox::drop-down {
    border: none
}

QComboBox::down-arrow {
    right: 8px;
    width: 12px;
    height: 12px;
    image: url(:svg/icons/chevron_down_16_filled_white.svg)
}

QScrollBar::vertical {
    width: 16px;
    background-color: #05FFFFFF;
    border-style: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar::handle::vertical {
    border-radius: 2px;
    margin: 6px;
    background: rgba(255, 255, 255, 0.25);
}

QScrollBar::handle::vertical::hover {
    border-radius: 3px;
    margin: 5px;
    background: rgba(255, 255, 255, 0.35);
}

QScrollBar::handle::vertical:pressed {
    background: rgba(255, 255, 255, 0.15);
}

QScrollBar::add-line::vertical,
QScrollBar::sub-line::vertical {
    border: none;
}

QScrollBar::horizontal {
    height: 16px;
    background-color: #05FFFFFF;
    border-style: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

QScrollBar::handle::horizontal {
    border-radius: 2px;
    margin: 6px;
    background: rgba(255, 255, 255, 0.25);
}

QScrollBar::handle::horizontal::hover {
    border-radius: 3px;
    margin: 5px;
    background: rgba(255, 255, 255, 0.35);
}

QScrollBar::handle::horizontal:pressed {
    background: rgba(255, 255, 255, 0.15);
}

QScrollBar::add-line::horizontal,
QScrollBar::sub-line::horizontal {
    border: none;
}

QSplitter {
    background-color: transparent;
    border: none;
    height: 6px
}

QSplitter::handle:vertical {
    height: 6px;
    background-color: transparent;
}

QGraphicsView {
    border: none;
    background-color: #2A2B2C;
}

QListWidget {
    color: #F0F0F0;
    outline: 0px;
    background: transparent;
    border: none
}

QListWidget::item {
    border: none;
    border-radius: 6px;
    padding: 4px 8px;
    /* margin-top: 2px;
    margin-bottom: 2px; */
    color: #F0F0F0;
}

QListWidget::item:hover {
    background-color: #10FFFFFF;
}

QListWidget::item:selected {
    /* background-color: #1AFFFFFF; */
    background-color: #9BBAFF;
    color: #000;
}

QListWidget#AppOptionsDialogTabListWidget {
    outline: 0px;
    background: transparent;
    border: none;
}

QListWidget#AppOptionsDialogTabListWidget::item {
    border: none;
    border-radius: 6px;
    padding: 6px 12px;
    margin-top: 3px;
    margin-bottom: 3px;
    color: #F0F0F0;
}

QListWidget#AppOptionsDialogTabListWidget::item:hover {
    background-color: #10FFFFFF;
}

QListWidget#AppOptionsDialogTabListWidget::item:selected {
    background-color: #9BBAFF;
    color: #000;
}

QMenu {
    padding: 4px;
    background-color: #202122;
    border: 1px solid #505050;
    border-radius: 6px;
    color: #F0F0F0;
}

QMenu::indicator {
    left: 6px;
    width: 20px;
    height: 20px;
}

QMenu::icon {
    left: 6px;
}

QMenu::item {
    background: transparent;
    color: #F0F0F0;
    padding: 5px 20px;
    min-width: 96px
}

QMenu[stats=checkable]::item,
QMenu[stats=icon]::item {
    padding-left: 12px;
}

QMenu::item:selected {
    background-color: #1AFFFFFF;
    border-style: none;
    border-radius: 4px;
}

QMenu::item:disabled {
    color: #909090;
    background-color: transparent;
}

QMenu::separator {
    height: 1px;
    background-color: #404040;
    margin: 2px 0;
}

QMenuBar {
    background-color: transparent;
    color: #F0F0F0;
}

QMenuBar::item {
    background-color: transparent;
    padding: 6px 12px;
    color: #f0f0f0;
    border-radius: 6px;
}

QMenuBar::item:selected {
    background-color: #10FFFFFF
}

QLineEdit {
    background: #10FFFFFF;
    border: 1px solid transparent;
    border-bottom: 1px solid #A0A0A0;
    border-radius: 4px;
    color: #F0F0F0;
    selection-color: #000;
    selection-background-color: #9BBAFF;
    padding: 3px;
}

QLineEdit:focus {
    background: #202122;
    border: 1px solid #505050;
    border-bottom: 2px solid #9BBAFF;
}

QAbstractSpinBox {
    background: #10FFFFFF;
    border: 1px solid transparent;
    border-bottom: 1px solid #A0A0A0;
    border-radius: 4px;
    color: #F0F0F0;
    selection-color: #000;
    selection-background-color: #9BBAFF;
    padding: 3px;
}

QAbstractSpinBox:focus {
    background: #202122;
    border: 1px solid #505050;
    border-bottom: 2px solid #9BBAFF;
}

QLabel {
    color: #F0F0F0;
}

QDialog {
    background: #202122;
}

QCheckBox {
    background: transparent;
    color: #F0F0F0;
}

QCheckBox::indicator:unchecked {
    border: 1px solid #505050;
    background: #2A2B2C;
}

QCheckBox::indicator:checked {
    border: 1px solid #505050;
    background: #9BBAFF;
}

QTabWidget {
    background: #232425;
    border: none
}

QTabWidget::pane {
    border: none;
}

QTableWidget::QTableView QTableCornerButton::section {
    background: #232425;
}

QTabBar {
    /* background-color: transparent; */
    background-color: #10FFFFFF;
    border-radius: 6px;
}

QTabWidget::tab-bar {
    alignment: center;
}

QTabBar::tab {
    color: #F0F0F0;
    background-color: transparent;
    padding: 6px 12px;
    /* border-bottom: 2px solid transparent; */
    border-radius: 6px;
}

QTabBar::tab:hover {
    /* border-bottom-color: #20FFFFFF; */
    background-color: #10FFFFFF;
}

QTabBar::tab:selected {
    /* color: #9BBAFF;
    border-bottom-color: #9BBAFF; */
    background-color: #9BBAFF;
    color: #000;
}

QTextEdit {
    background: #202122;
    border: 1px solid #505050;
    border-radius: 4px;
    color: #F0F0F0;
    selection-color: #000;
    selection-background-color: #9BBAFF;
    padding: 2px;
}

QPlainTextEdit {
    background: #202122;
    border: 1px solid #505050;
    border-radius: 4px;
    color: #F0F0F0;
    selection-color: #000;
    selection-background-color: #9BBAFF;
    padding: 2px;
}

QTableView {
    background-color: #202122;
    border: 1px solid #505050;
    border-radius: 4px;
    color: #F0F0F0;
    selection-color: #000;
    selection-background-color: #3A3B3C;
    padding: 4px;
}

QTableView>QHeaderView {
    background-color: transparent;
    background-color: transparent
}

QTableView>QHeaderView::section {
    background-color: transparent;
    border: none;
    color: #F0F0F0
}

QGroupBox {
    margin-top: 8px;
    padding-top: 8px;
    color: #F0F0F0;
    border: none;
    border-top: 1px solid #80606060;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    font-size: 10pt;
}

QCheckBox {
    border: none;
    color: #F0F0F0;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

QCheckBox::indicator:checked {
    /* border: 1px solid #9BBAFF; */
    border: none;
    border-radius: 4px;
    background-color: rgb(155, 186, 255);
    image: url(:/svg/checkbox/check-line-padded.svg);
}

QCheckBox::indicator:checked:hover {
    /* border: 1px solid #9BBAFF; */
    background-color: rgb(169, 196, 255);
}

QCheckBox::indicator:checked:pressed {
    /* border: 1px solid #9BBAFF; */
    background-color: rgb(139, 175, 255);
}

QCheckBox::indicator:checked:disabled {
    /* border: 1px solid #606060; */
    background-color: rgb(205, 205, 205);
}

QCheckBox::indicator:unchecked {
    border: none;
    /* border: 1px solid #80606060; */
    border-radius: 4px;
    background-color: rgb(64, 64, 64);
}

QCheckBox::indicator:unchecked:hover {
    background-color: rgb(72, 72, 72);
}

QCheckBox::indicator:unchecked:pressed {
    background-color: rgb(56, 56, 56);
}

QCheckBox::indicator:unchecked:disabled {
    border: 1px solid rgb(64, 64, 64);
}

TimelineView {
    background: rgb(28, 29, 30);
}

PhonemeView {
    background: rgb(28, 29, 30);
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
}

QWidget#tracksTimelineView {
    background: rgb(28, 29, 30);
    border-top-right-radius: 6px;
}

CommonGraphicsView {
    background: transparent;
}

LyricWrapView {
    background: transparent;
    border: 1px solid #505050;
    border-radius: 4px;
    padding: 2px;
}

DialogButtonBar {
    background: #30000000;
    border: none;
    /*border-top: 1px solid #1AFFFFFF;*/
}

QLabel#DialogTitleLabel {
    font-size: 13pt
}

CardView {
    border-radius: 8px;
    background-color: #10FFFFFF;
}

OptionsCard {
    /* background-color: #10FF0000; */
}

OptionsCard>QLabel#title {
    font-size: 11pt;
    color: #A0FFFFFF;
    /* background-color: #808000; */
}

OptionsCardItem {
    /* background-color: #1000FF00; */
}

OptionsCardItem>QLabel#title {
    font-size: 10pt;
