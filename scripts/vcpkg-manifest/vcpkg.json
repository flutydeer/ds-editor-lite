{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "dependencies": [
    "sdl2",
    "libsndfile",
    "stdutau",
    "qastool",
    "qnrbf",
    "qmsetup",
    {
      "name": "qwindowkit",
      "features": [
        "widgets"
      ]
    },
    "rpclib",
    "rtmidi",
    "interval-tree",
    "cpp-pinyin",
    "cpp-kana",
    "language-manager",
    "opendspx",
    "lyric-tab",
    "loadso",
    "syscmdline",
    "nlohmann-json",
    "yaml-cpp",
    "wavpack",
    "tinyexpr",
    "stduuid",
    "stdcorelib",
    "blake3",
    "sparsepp",
    "bit7z",
    {
      "name": "talcs",
      "features": [
        "device",
        "device-sdl",
        {
          "name": "device-asio",
          "platform": "windows & !(arm | uwp)"
        },
        "format",
        "midi",
        "dspx",
        "widgets"
      ]
    },
    "soxr",
    "wolf-midi",
    {
      "name": "qt-win32-direct-manipulate-helper",
      "platform": "windows"
    }
  ],
  "vcpkg-configuration": {
    "overlay-ports": [
      "../vcpkg/ports"
    ],
    "overlay-triplets": [
      "../vcpkg/triplets"
    ]
  }
}