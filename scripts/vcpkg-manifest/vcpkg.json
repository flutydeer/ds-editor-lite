{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "dependencies": [
    "sdl2",
    "libsndfile",
    "stdutau",
    "qastool",
    "qnrbf",
    "qmsetup",
    {
      "name": "qwindowkit",
      "features": [
        "widgets"
      ]
    },
    "rpclib",
    "rtmidi",
    "interval-tree",
    "cpp-pinyin",
    "cpp-kana",
    "language-manager",
    "opendspx",
    "lyric-tab",
    "yaml-cpp",
    "wavpack",
    "tinyexpr",
    "qbreakpad",
    {
      "name": "synthrt",
      "features": []
    },
    {
      "name": "talcs",
      "features": [
        "device",
        "device-sdl",
        {
          "name": "device-asio",
          "platform": "windows & !(arm | uwp)"
        },
        "format",
        "midi",
        "dspx",
        "widgets"
      ]
    },
    "soxr",
    "wolf-midi",
    {
      "name": "qt-win32-direct-manipulate-helper",
      "platform": "windows"
    }
  ],
  "features": {
    "cuda11": {
      "description": "Enable synthrt (dsinfer) CUDA 11.x support",
      "dependencies": [
        {
          "name": "synthrt",
          "features": [
            "cuda11"
          ]
        }
      ],
      "supports": "!osx & !arm & !arm64"
    },
    "cuda12": {
      "description": "Enable synthrt (dsinfer) CUDA 12.x support",
      "dependencies": [
        {
          "name": "synthrt",
          "features": [
            "cuda12"
          ]
        }
      ],
      "supports": "!osx & !arm & !arm64"
    }
  },
  "vcpkg-configuration": {
    "overlay-ports": [
      "../vcpkg/ports"
    ],
    "overlay-triplets": [
      "../vcpkg/triplets"
    ]
  }
}