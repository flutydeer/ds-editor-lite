{
  "name": "synthrt",
  "version": "0.0.2.1",
  "description": "SVS Synthesis Runtime Library",
  "homepage": "https://github.com/diffscope/synthrt",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "qmsetup",
    "nlohmann-json",
    "stduuid",
    "stdcorelib",
    "bit7z",
    "blake3",
    "sparsepp",
    "syscmdline"
  ],
  "features": {
    "cuda11": {
      "description": "Enable CUDA 11.x support",
      "supports": "!osx & !arm & !arm64"
    },
    "cuda12": {
      "description": "Enable CUDA 12.x support",
      "supports": "!osx & !arm & !arm64"
    }
  }
}
