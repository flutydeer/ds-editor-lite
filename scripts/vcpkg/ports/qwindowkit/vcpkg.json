{
  "name": "qwindowkit",
  "version": "1.4.0",
  "description": "Cross-platform frameless window framework for Qt. Support Windows, macOS, Linux.",
  "homepage": "https://github.com/stdware/qwindowkit",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "qmsetup",
      "host": true
    }
  ],
  "features": {
    "quick":{
      "description": "Build QWindowKit Quick module."
    },
    "widgets": {
      "description": "Build QWindowKit Widgets module."
    }
  },
  "default-features": ["quick"]
}
